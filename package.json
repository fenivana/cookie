{
  "name": "cookie.js",
  "version": "3.0.0",
  "description": "Cookie utility for browser",
  "main": "dist/cookie.js",
  "scripts": {
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/cookie.git"
  },
  "keywords": [
    "cookie",
    "browser"
  ],
  "author": "Jiang Fengming <fenix@noindoin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/cookie/issues"
  },
  "homepage": "https://github.com/fenivana/cookie#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.5.2",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "4.1.0",
    "eslint-config-enough": "0.2.7"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
